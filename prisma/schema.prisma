generator db {
  provider = "go run github.com/prisma/prisma-client-go"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model movies {
  id          Int      @id @default(autoincrement())
  title       String?
  subtitle    String?
  other       String?
  desc        String?
  year        String?
  area        String?
  tag         String?
  star        String?
  comment     String?
  quote       String?
  created_at  DateTime @default(now())
  updated_at  DateTime @default(now())
}
